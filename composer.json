{
  "name": "remils/laravel-recaptcha",
  "description": "Google reCaptcha V3 for Laravel 7.x, 8.x, 9.x",
  "version": "1.0.0",
  "keywords": [
    "google",
    "recaptcha",
    "laravel"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Remils\\LaravelRecaptcha\\": "src/"
    }
  },
  "homepage": "https://github.com/remils/laravel-recaptcha",
  "authors": [
    {
      "name": "Sergey Zatulivetrov",
      "email": "remils@mail.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2.5|^8.0"
  },
  "require-dev": {
    "laravel/framework": "^7.0|^8.0|^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Remils\\LaravelRecaptcha\\RecapthaProvider"
      ]
    }
  }
}
